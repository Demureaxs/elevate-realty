{
    "compilerOptions": {
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "paths": {
        "@/*": ["./src/*"],
        "@middleware/*": ["./src/middleware/*"]
      }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/global.d.ts", "src/middleware/*"],
    "exclude": ["node_modules"]
  }
  